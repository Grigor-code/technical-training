 <record id="test_dispetcher_one_tree" model="ir.ui.view">
        <field name="name">org.bid.view.tree</field>
        <field name="model">org.bid</field>
        <field name="arch" type="xml">
            <tree string="Properties" >
                <field name="name"/>
                <field name="task_ids"/>
                <field name="status"/>
                <field name="planned_date" optional="hide"/>
                <field name="actual_date" optional="hide"/>
            </tree>
        </field>
    </record>
    <record id="test_dispetcher_one_form" model="ir.ui.view">
        <field name="name">org.bid.view.form</field>
        <field name="model">org.bid</field>
        <field name="arch" type="xml">
            <form string="detail">
                <sheet>
                    <div class="oe_title">
                        <h1>
                            <field name="name"/>
                        </h1></div>
                    <group>
                        <group>
                            <field name="description" />
                            <field name="obj"/>
                        </group>
                        
                    </group>
                    <notebook>
                        <page name="date" string="Date">
                            <group>
                                <field name="status"/>
                                <field name="date_create"/>
                                <field name="planned_date"/>
                                <field name="actual_date"/>
                            
                                
                            </group>
                        </page>
                        <page string="Задачи и компетенции">
                            <group>
                              <field name="task_ids"/>
                              <field name="competence"/>
                            </group>
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>



</odoo>